<h4>Create User</h4>
<br>
<form action="<%= users_url %>" method="post">
  <label>Email</label>
  <input type="text"
         name="user[email]"
         id="user_email"
         value="<%= @user.email %>">
  <br>
  <label>Password</label>
  <input type="password"
         name="user[password]"
         id="user_password">
  <br>
  <input type="submit">
</form>
